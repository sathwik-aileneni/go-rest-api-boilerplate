{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start/Restart Docker Services & Debug",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 2345,
      "host": "127.0.0.1",
      "apiVersion": 2,
      "debugAdapter": "legacy",
      "substitutePath": [
        {
          "from": "${workspaceFolder}",
          "to": "/app"
        }
      ],
      "showLog": true,
      "trace": "verbose",
      "preLaunchTask": "docker-restart-dev"
    }
  ]
}
